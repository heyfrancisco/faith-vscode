{
  "name": "Faith Theme",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#333300",
    "editor.selectionBackground": "#73597ee0",
    "editorCursor.foreground": "#ffffff",
    "editorWhitespace.foreground": "#404040",
    "editorBracketHighlight.foreground1": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": "text",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "source",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9933CC"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#3387CC"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#CC7833",
        "fontStyle": ""
      }
    },
    {
      "scope": "meta.preprocessor.c",
      "settings": {
        "foreground": "#D0D0FF",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.control.import",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "source comment.block",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#66CC33"
      }
    },
    {
      "scope": "string constant.character.escape",
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "scope": "string.interpolated",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#CCCC33"
      }
    },
    {
      "scope": "string.literal",
      "settings": {
        "foreground": "#CCCC33"
      }
    },
    {
      "scope": "string.interpolated constant.character.escape",
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#C83730",
        "fontStyle": ""
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "scope": "variable.other.block.ruby",
      "settings": {
        "foreground": "#C83730",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.separator.inheritance.ruby",
      "settings": {
        "foreground": "#CC7833",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#C83730"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#ff7985" // JS: let, const, var
      }
    },
    {
      "scope": "variable.other.readwrite, variable.other",
      "settings": {
        "foreground": "#00e1ff" // JS: Variable names
      }
    }
  ]
}
